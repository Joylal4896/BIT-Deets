    <div class="container-fluid" id="feedback-cover">
    <div><div class="feedback-caption">Feedback Form</div>
    <div class="logo-feedback"><img src="/img/deets.png" height="auto" width="100px"></div>
    </div>
    <hr>
    <div class ="form-horizontal">
    <center><div id="main-container" class="container">
      <div class="row">
        <div class="col-lg-6">
        
  <% flash.each do |name, msg| -%>
    <strong><%= content_tag :div, msg, class: 'alert alert-success'%></strong>
  <% end %>
  </div></div></div><div></center>

    <%= form_for(@feedback, :url => { :action => :create }) do |f| %>
      <div class="form-group">
        <label for="inputName" class="col-lg-3 control-label">Name</label>
        <div class="col-lg-7">
          <%= f.text_field :name, class: 'form-control'%>
        </div>
      </div>

      <div class="form-group">
        <label for="inputRoll" class="col-lg-3 control-label">Roll no.</label>
        <div class="col-lg-7">
          <%= f.text_field :rollno, class: 'form-control'%>
        </div>
      </div>

      

      

      <div class="form-group">
        <label for="inputPhone" class="col-lg-3 control-label">Phone no.</label>
        <div class="col-lg-7">
          <%= f.text_field :phoneno, class: 'form-control'%>
        </div>
      </div>

      <div class="form-group">
        <label for="feedback" class="col-lg-3 control-label">Feedback / Suggestions</label>
        <div class="col-lg-7">
          <%= f.text_area :feedback, class: 'form-control'%>
        </div>
      </div>

      <div class="form-group">
        <label for="more" class="col-lg-3 control-label">What more you want us to put on the site</label>
        <div class="col-lg-7">
          <%= f.text_area :putmore, class: 'form-control'%>
        </div>
      </div>

      

      <div class="form-group">
        <div class="col-lg-offset-3 col-lg-7">
          <%= f.submit "Submit", class: 'btn btn-default'%>
        </div>
      </div>
    <% end %>
    </div>
