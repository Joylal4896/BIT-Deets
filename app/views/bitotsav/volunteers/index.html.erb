<p id="notice"><%= notice %></p>

<div class="page-header"><center><h1>Listing Bitotsav Volunteers</h1></center></div>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Role / Work Assigned</th>
      <th>Rating</th>
      <th>Active</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bitotsav_volunteers.each do |bitotsav_volunteer| %>
      <tr>
        <td><%= bitotsav_volunteer.Name %></td>
        <td><%= bitotsav_volunteer.Role %></td>
        <td><%= bitotsav_volunteer.Rating %></td>
        <td><%= bitotsav_volunteer.Activity %></td>
        <td><%= bitotsav_volunteer.Team %></td>
        <td><%= link_to 'Show', bitotsav_volunteer %></td>
        <td><%= link_to 'Edit', edit_bitotsav_volunteer_path(bitotsav_volunteer) %></td>
        <td><%= link_to 'Destroy', bitotsav_volunteer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Volunteer', new_bitotsav_volunteer_path %>
