<p id="notice"><%= notice %></p>

<div class="page-header"><center><h1>Listing Bitotsav Organisers</h1></center></div>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Role / Work Assigned </th>
      <th>Rating</th>
      <th>Active</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bitotsav_organisers.each do |bitotsav_organiser| %>
      <tr>
        <td><%= bitotsav_organiser.Name %></td>
        <td><%= bitotsav_organiser.Role %></td>
        <td><%= bitotsav_organiser.Rating %></td>
        <td><%= bitotsav_organiser.Activity %></td>
        <td><%= bitotsav_organiser.Team %></td>
        <td><%= link_to 'Show', bitotsav_organiser %></td>
        <td><%= link_to 'Edit', edit_bitotsav_organiser_path(bitotsav_organiser) %></td>
        <td><%= link_to 'Destroy', bitotsav_organiser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organiser', new_bitotsav_organiser_path %>
